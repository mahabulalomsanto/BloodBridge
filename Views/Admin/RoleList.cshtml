@using Microsoft.AspNetCore.Identity
@model IEnumerable<IdentityRole>

<h2>Role List</h2>
<table class="table">
    <thead>
        <tr>
            <th>Role Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Name</td>
            </tr>
        }
    </tbody>
</table>
<a asp-controller="Admin" asp-action="AddRole" class="btn btn-primary">Add New Role</a>
